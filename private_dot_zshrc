## [Completion]
## Completion scripts setup. Remove the following line to uninstall

# Docker completions path (this needs to be set before compinit)
fpath=(/Users/yeongshengtan/.docker/completions $fpath)

# Ensure completion system is available
#if ! command -v compdef &>/dev/null; then
#  autoload -Uz compinit
#  compinit -u
#fi

# Now load all the deferred completions
if command -v fasd &>/dev/null; then
  fasd_cache="$HOME/.fasd-init-zsh-comp"
  if [ "$(command -v fasd)" -nt "$fasd_cache" -o ! -s "$fasd_cache" ]; then
    fasd --init zsh-ccomp zsh-ccomp-install zsh-wcomp zsh-wcomp-install >| "$fasd_cache"
  fi
  [ -f "$fasd_cache" ] && source "$fasd_cache"
  unset fasd_cache
fi

# fzf completions
if [ -e ~/.fzf ] && [ -f ~/.fzf/shell/completion.zsh ]; then
  source ~/.fzf/shell/completion.zsh
fi

if [ -f '/Users/yeongshengtan/google-cloud-sdk/completion.zsh.inc' ]; then 
  source '/Users/yeongshengtan/google-cloud-sdk/completion.zsh.inc'
fi

if command -v ngrok &>/dev/null; then
  eval "$(ngrok completion)"
fi

# Additional completion sources
[[ -f ~/.config/tabtab/zsh/__tabtab.zsh ]] && . ~/.config/tabtab/zsh/__tabtab.zsh || true
[[ -f /Users/yeongshengtan/.dart-cli-completion/zsh-config.zsh ]] && . /Users/yeongshengtan/.dart-cli-completion/zsh-config.zsh || true

## [/Completion]
